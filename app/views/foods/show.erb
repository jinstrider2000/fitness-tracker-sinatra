<div class="container">
  <div class="row text-center">
    <img class="img-responsive index-icon" src="<%=url("images/food_icon.png")%>" alt="Food Icon">
    <h2><%=@main_heading%></h2>
  </div>
  <div class="row text-center">
    <h3><%=print_time(@meal.created_at)%></h3>
    <div class="col-sm-6">
      <h4><%=@meal.name%></h4>
    </div>
    <div class="col-sm-6">
      <h4><%=@meal.calories%></h4>
    </div>
  </div>
</div>